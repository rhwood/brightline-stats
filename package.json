{
  "name": "brightline-stats",
  "version": "0.0.1",
  "description": "Brightline ridership and revenue visualizer",
  "repository": "https://github.com/rhwood/brightline-stats.git",
  "homepage": "https://rhwood.github.io/brightline-stats",
  "license": "Apache-2.0",
  "private": true,
  "scripts": {
    "prebuild": "npm run type-check && npm run clean",
    "build": "webpack --config webpack.prod.js",
    "build:gh": "ASSET_PATH=/brightline-stats/ BASE_PATH=/brightline-stats/ webpack --config webpack.prod.js",
    "start": "sirv dist --cors --single --host --port 8080",
    "start:dev": "webpack serve --color --progress --config webpack.dev.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "eslint": "eslint ./src/",
    "lint": "npm run eslint",
    "format": "prettier --check --write ./src/**/*.{tsx,ts}",
    "type-check": "tsc --noEmit",
    "ci-checks": "npm run type-check && npm run lint && npm run test:coverage",
    "build:bundle-profile": "webpack --config webpack.prod.js --profile --json > stats.json",
    "bundle-profile:analyze": "npm run build:bundle-profile && webpack-bundle-analyzer ./stats.json",
    "clean": "rimraf dist"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.9",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.21.0",
    "@patternfly/react-charts": "8.2.2",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.2.0",
    "@testing-library/user-event": "14.6.1",
    "@types/jest": "29.5.14",
    "@types/react-router-dom": "5.3.3",
    "@typescript-eslint/eslint-plugin": "8.31.0",
    "@typescript-eslint/parser": "8.31.0",
    "copy-webpack-plugin": "13.0.0",
    "css-loader": "7.1.2",
    "css-minimizer-webpack-plugin": "7.0.2",
    "csv-loader": "3.0.5",
    "dotenv-webpack": "8.1.0",
    "eslint": "9.26.0",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "globals": "^16.0.0",
    "html-webpack-plugin": "5.6.3",
    "imagemin": "9.0.1",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "mini-css-extract-plugin": "2.9.2",
    "papaparse": "5.5.2",
    "postcss": "8.5.3",
    "prettier": "3.5.3",
    "prop-types": "15.8.1",
    "raw-loader": "4.0.2",
    "react-axe": "3.5.4",
    "react-router-dom": "7.6.0",
    "regenerator-runtime": "0.14.1",
    "rimraf": "6.0.1",
    "sirv-cli": "3.0.1",
    "style-loader": "4.0.0",
    "svg-url-loader": "8.0.0",
    "terser-webpack-plugin": "5.3.14",
    "ts-jest": "29.3.4",
    "ts-loader": "9.5.2",
    "tsconfig-paths-webpack-plugin": "4.2.0",
    "tslib": "2.8.1",
    "typescript": "5.8.3",
    "url-loader": "4.1.1",
    "webpack": "5.99.7",
    "webpack-bundle-analyzer": "4.10.2",
    "webpack-cli": "6.0.1",
    "webpack-dev-server": "5.2.1",
    "webpack-merge": "6.0.1"
  },
  "dependencies": {
    "@patternfly/react-core": "6.2.2",
    "@patternfly/react-icons": "6.2.2",
    "@patternfly/react-styles": "6.2.2",
    "react": "18",
    "react-dom": "18"
  }
}
